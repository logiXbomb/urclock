{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
    	"react/jsx-filename-extension": 0,
      "react/jsx-wrap-multilines": 0,
      "react/jsx-closing-tag-location": 0,
      "arrow-parens": 0
    },
    "plugins": ["flowtype"],
    "env": {
    	"browser": true,
	"node": true
    }
}
